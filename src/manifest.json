{
  "manifest_version": 2,
  "name": "ze.tt-remover",
  "description": "This extension for Mozilla Firefox hides ze.tt content on zeit.de",
  "version": "0.0.1",
  "icons": {
    "64": "icons/icon-64.png",
    "96": "icons/icon-96.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.zeit.de/*"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ]
}